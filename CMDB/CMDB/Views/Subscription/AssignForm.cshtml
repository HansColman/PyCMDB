@model CMDB.API.Models.SubscriptionDTO
@{
    Layout = "_Layout";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered">
    <thead class="thead-light">
        <tr>
            <th>Name</th>
            <th>UserID</th>
            <th>E Mail</th>
            <th>Language</th>
            <th>Type</th>
            <th>State</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Html.DisplayTextFor(item => item.Identity.Name)</td>
            <td>@Html.DisplayFor(item => item.Identity.UserID)</td>
            <td>@Html.DisplayFor(item => item.Identity.EMail)</td>
            <td>@Html.DisplayFor(item => item.Identity.Language.Description)</td>
            <td>@Html.DisplayFor(item => item.Identity.Type.Description)</td>
            <td>@Html.DisplayFor(item => item.Identity.State)</td>
        </tr>
    </tbody>
</table>
<h3>Subscription overview</h3>
<table class="table table-striped table-bordered">
    <thead class="thead-light">
        <tr>
            <th>Type</th>
            <th>Phone number</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td class="small">@Html.DisplayFor(item => item.SubscriptionType.AssetCategory.Category) @Html.DisplayFor(item => item.SubscriptionType.Provider) @Html.DisplayFor(item => item.SubscriptionType.Type)</td>
            <td class="small">@Html.DisplayFor(item => item.PhoneNumber)</td>
        </tr>
    </tbody>
</table>
<partial name="_SignForm" />
