@model CMDB.Models.Admin;
@{
    Layout = "_Layout";
}
@if ((bool)ViewData["DeleteAccess"])
{
    <h2>@ViewData["Title"]</h2>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.Account.UserID)</th>
                <th>@Html.DisplayNameFor(model => model.Level)</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Html.DisplayTextFor(model => model.Account.UserID)</td>
                <td>@Html.DisplayTextFor(model => model.Level)</td>
            </tr>
        </tbody>
    </table>
    <partial name="_deleteForm" />
}
else
{
    <p>You do not access to this page</p>
}